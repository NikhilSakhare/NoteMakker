<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SaveNoteServlet</servlet-name>
  	<display-name>SaveNoteServlet</display-name>
  	<description></description>
  	<servlet-class>com.servelet.SaveNoteServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteServelet</servlet-name>
  	<display-name>DeleteServelet</display-name>
  	<description></description>
  	<servlet-class>com.servelet.DeleteServelet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateServelet</servlet-name>
  	<display-name>UpdateServelet</display-name>
  	<description></description>
  	<servlet-class>com.servelet.UpdateServelet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SaveNoteServlet</servlet-name>
  	<url-pattern>/SaveNoteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteServelet</servlet-name>
  	<url-pattern>/DeleteServelet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateServelet</servlet-name>
  	<url-pattern>/UpdateServelet</url-pattern>
  </servlet-mapping>
</web-app>
